generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @id @default(autoincrement())
  firstname String
  lastname  String
  createdAt DateTime  @default(now())
  email     String    @unique
  password  String
  username  String    @unique
  dob       DateTime?
}
